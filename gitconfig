[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  unstage = reset HEAD --
  last = log -1 HEAD
  dr = log --oneline --graph --all --decorate
  rbm = rebase upstream/master
  rbd = rebase upstream/develop

